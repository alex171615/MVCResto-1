@using RestoMVC.Core;
@model IEnumerable<Restaurante>
@{
    ViewData["Title"] = "Restaurantes";
}

<body>
    <table>
        <tbody>
            @foreach (Restaurante restaurante in Model)
            {
                <tr>
                    <td>
                        <a asp-controller="Restaurante" asp-action="Index" asp-route-id="@restaurante.Id"><li style="color: black;">@restaurante.Nombre</li></a> 
                    </td>
                </tr>
            }
        </tbody>
    </table>
</body>